{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run cds-deploy",
      "runtimeExecutable": "cds-deploy",
      "cwd": "${workspaceFolder}",
      "args": []
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run cds",
      "runtimeExecutable": "cds",
      "cwd": "${workspaceFolder}",
      "args": []
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "Run cds",
      "cwd": "${workspaceFolder}//capmapp"
    },
    {
      "name": "Praveen Modules Debug",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}//capmapp",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "command": "cds watch"
    }
  ]
}
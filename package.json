{
  "name": "capmapp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2.3.2",
    "@sap/cds": "^9",
    "@sap/cds-dk": "^9.3.2",
    "@sap/xssec": "^4.10.0",
    "express": "^4",
    "ig": "^0.0.5"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-product": "cds watch --open productset.product/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-project1": "cds watch --open com.fin.project1/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-project01": "cds watch --open productset.project01/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-pos": "cds watch --open com.pos.pos/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/product",
    "app/project1",
    "app/project01",
    "app/project1",
    "app/project1",
    "app/pos",
    "app/project1"
  ],
  "cds": {
    "requires": {
      "db": "hana",
      "static": {
        "kind": "static",
        "folder": "app"
      },
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[development]": {
          "strategy": "mocked",
          "users": {
            "praveen": {
              "roles": ["Viewer"],
              "userAttributes": {
                "BankName" : ["My Bank of San Francisco"]
              }
            },
            "kumar": {
              "roles": ["Viewer","Admin"],
              "userAttributes": {
                "BankName" : ["My Bank of London","My Bank of New York"]
              }
            }
          }
        },
        "[production]": {
          "strategy": "JWT"
        }
      }
      
    }
  }
}
